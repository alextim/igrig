{
  "name": "igrig",
  "description": "Static website for `IGRIG`",
  "private": true,
  "version": "0.2.0",
  "license": "MIT",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "blog",
    "landing",
    "i18n",
    "eslint",
    "emotion"
  ],
  "main": "n/a",
  "homepage": "https://github.com/alextim/igrig",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextim/igrig.git"
  },
  "bugs": {
    "url": "https://github.com/alextim/igrig/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ua": "cross-env LOCALES=ua ONLY=/ gatsby build",
    "dev": "gatsby develop -o",
    "clean": "gatsby clean",
    "serve": "gatsby serve -o",
    "lint": "eslint src/",
    "lint:write": "eslint --debug src/ --fix",
    "prettier": "prettier --write src/**/*.{js, jsx}",
    "stylelint": "stylelint --syntax css-in-js src/**/*.{js,jsx,ts,tsx,css,scss}",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx,json,md}'",
    "git:tracked": "git ls-tree -r master --name-only",
    "git:log": "git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "test": "cross-env NODE_ENV=test yarn run eslint",
    "testbuild": "yarn test && yarn build",
    "ncu": "ncu -u --timeout 150000",
    "trans": "node scripts/translations-json-to-yaml",
    "upd": "scripts/content-update.bat",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@alextim/at-seo": "^0.4.5",
    "@alextim/at-site-core": "^0.8.6",
    "@alextim/at-sitemap": "^0.4.0",
    "@alextim/csp": "^0.1.2",
    "@alextim/i18n-utils": "^0.1.0",
    "@alextim/slugify": "^0.1.0",
    "@alextim/translit": "^0.1.1",
    "@alextim/utils": "^0.1.0",
    "@emotion/react": "^11.7.1",
    "@fastly/performance-observer-polyfill": "^2.0.0",
    "emotion-theming": "^11.0.0",
    "gatsby": "^4.5.3",
    "gatsby-plugin-emotion": "^7.5.0",
    "gatsby-plugin-feed": "^4.5.0",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-netlify": "^3.14.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-remove-generator": "^1.2.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-remark-images": "^6.5.2",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.5.2",
    "gatsby-transformer-remark": "^5.5.2",
    "gatsby-transformer-sharp": "^4.5.0",
    "gatsby-transformer-yaml": "^4.5.0",
    "medium-zoom": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "dotenv": "^14.2.0",
    "@emotion/eslint-plugin": "^11.7.0",
    "@testing-library/react-hooks": "^7.0.2",
    "babel-preset-gatsby": "^2.5.2",
    "cross-env": "^7.0.3",
    "eslint": "8.7.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-react-svg": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.1",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "preval.macro": "^5.0.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0"
  },
  "comments": {
    "content/data/locales/translations/src/*.json": [
      "yarn translations:json2yaml",
      "git add ."
    ],
    "browserslist": [
      ">0.25%",
      "not dead"
    ],
    "audit-ci": "",
    "gatsby-plugin-webpack-bundle-analyser-v2": "",
    "license-checker": "",
    "react-image-lightbox": "",
    "lazysizes": "",
    "@loadable/component": "",
    "disable-scroll": "",
    "prettier-eslint-cli": "",
    "git": "git fsck, git gc, git prune"
  }
}
